Options +FollowSymLinks
# Turn off MultiViews
Options -MultiViews

# Rewrite engine setup
RewriteEngine On

#### Rewrite rules for cdvno/test
# text/turtle
RewriteCond %{HTTP:Accept} text/turtle [NC] 
RewriteRule ^(test)$  https://informasjonsforvaltning.github.io/cdvno/files/$1.ttl [R=302,L]
# Norwegian Bokm√•l
RewriteCond %{HTTP:Accept-Language} nb [NC]
RewriteRule ^(test)$ https://informasjonsforvaltning.github.io/cdvno/files/$1-nb [R=302,L]
# Norwegian Nynorsk
RewriteCond %{HTTP:Accept-Language} nn [NC]
RewriteRule ^(test)$ https://informasjonsforvaltning.github.io/cdvno/files/$1-nb [R=303,L]
# Norwegian Norsk
RewriteCond %{HTTP:Accept-Language} no [NC]
RewriteRule ^(test)$ https://informasjonsforvaltning.github.io/cdvno/files/$1-nb [R=303,L]
# English
RewriteCond %{HTTP:Accept-Language} en [NC]
RewriteRule ^(test)$ https://informasjonsforvaltning.github.io/cdvno/files/$1-en [R=302,L]
# English as default
RewriteRule ^(test)$ https://informasjonsforvaltning.github.io/cdvno/files/$1-en [R=303,L] 

#### redirect to default cdvno
RewriteRule ^cdvno https://informasjonsforvaltning.github.io/cdvno/ [R=302,L]